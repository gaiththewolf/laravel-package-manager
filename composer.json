    "keywords": ["tracker", "tracking", "packages", "manager", "package"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mr.Wolf",
            "email": "gaiththewolf@gmail.com",
            "role": "Creator & Designer"
        }
